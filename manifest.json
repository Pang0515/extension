{
  "manifest_version": 3,
  "name": "HKweather",
  "description": "You can check the weather while using browser!",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html",
    "default_icon": "128.png"
  },
  "content_scripts": [
    {
      "js": ["popup.js"],
      "css" : ["styls.css"],
      "matches": ["https://developer.chrome.com/docs/extensions/*"]
    }
  ]
}
